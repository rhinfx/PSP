<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.org/dtd/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
  <table-filter match-catalog="menus" match-name="authors"/>
  <table-filter match-catalog="menus" match-name="dishes"/>
  <table-filter match-catalog="menus" match-name="dishes_authors"/>
  <table-filter match-catalog="menus" match-name="dishes_ingredients"/>
  <table-filter match-catalog="menus" match-name="ingredients"/>
  <table-filter match-catalog="menus" match-name="menus"/>
  <table-filter match-catalog="menus" match-name="users"/>
  <table-filter match-catalog="menus" match-name="users_menus"/>
</hibernate-reverse-engineering>